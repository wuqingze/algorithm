        d1[label="D height:h"]
        e1[label="E height:h+1" style=filled]
        e1_l[label="E_l"];
        e1_r[label="E_r"];
        a1->b1;
        a1->c1;
        b1->d1;
        b1->e1;
        e1->e1_l;
        e1->e1_r;
