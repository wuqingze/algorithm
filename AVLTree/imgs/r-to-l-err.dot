digraph G {

    graph [fontsize=10 fontname="Verdana" compound=true];

    subgraph cluster_0 {
        a1[label="A"]
        b1[label="B height:h+2"]
        c1[label="C height:h"]
        d1[label="D height:h"]
        e1[label="E height:h+1" style=filled]
        e1_l[label="E_l"];
        e1_r[label="E_r"];
        a1->b1;
        a1->c1;
        b1->d1;
        b1->e1;
